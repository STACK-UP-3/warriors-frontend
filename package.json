{
  "name": "warriors-frontend",
  "version": "1.0.0",
  "description": "Web application frontend built with React, for Andela Kigali Stack-Up 3",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --coverage",
    "start": "npm run build",
    "start:dev": "npm run build:dev",
    "build": "webpack -p && webpack-dev-server --content-base src/",
    "build:dev": "webpack -d && webpack-dev-server --content-base src/ --inline --hot",
    "dev": "webpack --mode development --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/STACK-UP-3/warriors-frontend.git"
  },
  "keywords": [
    "react",
    "barefoot nomad"
  ],
  "author": "Warriorz Team - Andela Kigali Stack-Up 3",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/STACK-UP-3/warriors-frontend/issues"
  },
  "homepage": "https://github.com/STACK-UP-3/warriors-frontend",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "materialize-css": "^1.0.0-rc.2",
